<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
              version="2.2">
    <application>
        <locale-config>
            <default-locale>tr</default-locale>
        </locale-config>
        <resource-bundle>
            <base-name>langprop</base-name>
            <var>lang</var>
        </resource-bundle>
    </application>
    
    <navigation-rule>
        <from-view-id>/register/register.xhtml</from-view-id>
        
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/registersuccess.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>fail</from-outcome>
            <to-view-id>/error.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/login/login.xhtml</from-view-id>
        
        <navigation-case>
            <from-outcome>user page</from-outcome>
            <to-view-id>/user/usermain.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>doctor page</from-outcome>
            <to-view-id>/doctor/doctormain.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>admin page</from-outcome>
            <to-view-id>/admin/admin.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>fail</from-outcome>
            <to-view-id>/error.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/user/usermain.xhtml</from-view-id>
        
        <navigation-case>
            <from-outcome>thanks</from-outcome>
            <to-view-id>/thanks.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>details</from-outcome>
            <to-view-id>/details/detail.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/admin/admin.xhtml</from-view-id>
        
        <navigation-case>
            <from-outcome>thanks</from-outcome>
            <to-view-id>/thanks.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/doctor/doctormain.xhtml</from-view-id>
        
        <navigation-case>
            <from-outcome>thanks</from-outcome>
            <to-view-id>/thanks.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/details/detail.xhtml</from-view-id>
        
        <navigation-case>
            <from-outcome>dabtipahib</from-outcome>
            <to-view-id>/details/dabtipahib.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>hepatitisa</from-outcome>
            <to-view-id>/details/hepatitisa.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>hepatitisb</from-outcome>
            <to-view-id>/details/hepatitisb.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>kpa</from-outcome>
            <to-view-id>/details/kpa.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>kkk</from-outcome>
            <to-view-id>/details/kkk.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>opa</from-outcome>
            <to-view-id>/details/opa.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>rva</from-outcome>
            <to-view-id>/details/rva.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>others</from-outcome>
            <to-view-id>/details/othervaccines.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
    </navigation-rule>
</faces-config>